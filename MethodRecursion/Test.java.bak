import java.util.Scanner;

class  Test
{
	public static int count(int n)
	{
		if(n==0) return 0;
		int count =0;
		count(n/10);
		count++;
		return count;
		
	}
	public static void main(String[] args) 
	{
		System.out.println("Enter your number!");
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		System.out.println(count(n));
	}
}
