class NumberPattern5
{
	public static void main(String[] args) 
	{
		int n=5;
		for (int i=1;i<=n;i++)
		{
			
			if (i%2!=0)
			{
				int x=1,l=3;
				for (int j=1;j<=i;j++)
				{
					System.out.print(x+"\t");
					x+=l;
					
				}
				l++;
				System.out.println();
			}
			else{
				int y=(i*(i+1))/2;
				for (int j=1;j<=i;j++)
				{
					System.out.print(y+"\t");
					y--;
				}
				System.out.println();
			}
			
		}
	}
}
